{
  "expo": {
    "name": "BlueCrew",
    "slug": "bluecrew",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bluecrew",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "BlueCrew - Professional project management application with role-based access control for admins, project managers, and clients.",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bluecrew.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.bluecrew.app",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "name": "BlueCrew",
      "shortName": "BlueCrew",
      "lang": "en",
      "scope": "/",
      "themeColor": "#236ecf",
      "backgroundColor": "#fefce8",
      "display": "standalone",
      "orientation": "portrait"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "537b7521-f324-4bef-9242-017c14a43971"
      },
      "router": {}
    },
    "owner": "44network"
  }
}
